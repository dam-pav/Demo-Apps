<?xml version="1.0" encoding="UTF-8"?>
<xs:schema id="PODO-Common-1" targetNamespace="http://edavki.durs.si/Documents/Schemas/PODO-Common-1.xsd" elementFormDefault="qualified" xmlns="http://edavki.durs.si/Documents/Schemas/PODO-Common-1.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="NER">
    <xs:annotation>
      <xs:documentation>
        Poročilo o dohodkih izplačanih nerezidentom, ki so vključeni v obračun davčnih odtegljajev
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Total_F8" type="Decimal14_2Type" minOccurs="0">
          <xs:annotation>
            <xs:documentation>Skupaj znesek dohodka</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Total_F9" type="Decimal14_2Type" minOccurs="0">
          <xs:annotation>
            <xs:documentation>Skupaj davčni odtegljaj</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Total_F10" type="Decimal14_2Type" minOccurs="0">
          <xs:annotation>
            <xs:documentation>Skupaj prispevki za socialno varnost delojemalca</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="NERItem" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="F1_Name" type="xs:string" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Ime in priimek prejemnika</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="F2_Address" type="xs:string" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Podatki o bivališču</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="F3_ResidentCountry" type="xs:string" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Oznaka za rezidentstvo</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="F4_TaxNumber" type="xs:int" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Davčna številka</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="F5_BirthDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Datum rojstva</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="F6_Benefits" type="xs:string" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Nerezident uveljavlja ugodnosti iz mednarodnih pogodb</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="F7_OtherBenefits" type="xs:string" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Nerezident uveljavlja ugodnosti iz drugih mednarodnih pogodb</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="F8" type="Decimal14_2Type" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Znesek dohodka</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="F9" type="Decimal14_2Type" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Davčni odtegljaj</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="F10" type="Decimal14_2Type" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Obvezni prispevki za socialno varnost delojemalca</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="AnalyticalData">
    <xs:annotation>
      <xs:documentation>Individualni del REK obrazca</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="AnalyticalDataItem" minOccurs="1" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation>Individualni del REK obrazca</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="PaymentID" minOccurs="0" type="xs:int">
                <xs:annotation>
                  <xs:documentation>Zaporedna številka izplačila</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A001" minOccurs="0" type="xs:int">
                <xs:annotation>
                  <xs:documentation>Davčna številka</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A002" minOccurs="0" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Identifikacijska številka</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:choice>
                <xs:element name="A003" minOccurs="1" type="xs:string">
                  <xs:annotation>
                    <xs:documentation>Ime in priimek</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:sequence>
                  <xs:element name="A003i" minOccurs="1" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>Ime</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="A003p" minOccurs="0" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>Priimek</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:choice>
              <xs:element name="A004" minOccurs="1">
                <xs:annotation>
                  <xs:documentation>Oznaka za rezidentstvo</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="1"/>
                    <xs:enumeration value="R">
                      <xs:annotation>
                        <xs:documentation>Rezident</xs:documentation>
                      </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="N">
                      <xs:annotation>
                        <xs:documentation>Nerezident</xs:documentation>
                      </xs:annotation>
                    </xs:enumeration>
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="A004a" minOccurs="0" type="typeCountryCode">
                <xs:annotation>
                  <xs:documentation>Država rezidenstva</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A004b" minOccurs="0" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Naslov nerezidenta – ulica</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A004c" minOccurs="0" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Naslov nerezidenta – kraj</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A004d" minOccurs="0" type="typeCountryCode">
                <xs:annotation>
                  <xs:documentation>Naslov nerezidenta – država</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A005" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Uveljavljanje ugodnosti iz mednarodnih pogodb</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A011" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Delavec napoten na delo v tujino, za katerega velja drugi odstavek 144. člena ZPIZ-2</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A011a" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Napotitev na čezmejno opravljanje dela (45.a čl. ZDoh-2)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A011b" minOccurs="0" type="xs:date">
                <xs:annotation>
                  <xs:documentation>Datum prve napotitve</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A011c" minOccurs="0" type="xs:int">
                <xs:annotation>
                  <xs:documentation>Zaporedna številka meseca uveljavljanja posebne davčne osnove</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A012" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Napotitev na delo v tujino - javni uslužbenec</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A013" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Zavezanec, opravlja delo na trgovski ladji dolge plovbe (drugi odst. 42. čl. ZDoh-2)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A014" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Invalid nad predpisano kvoto</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A015" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Oseba, zaposlena po programu za zagotavljanje sredstev družbam za sofinanciranje plač</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A016" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Delavec, ki prejema nadomestilo plače po predpisih o pokojninskem in invalidskem zavarovanju</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A017" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Dopolnjena starost 60 let (156. čl. ZPIZ-2)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A018" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Delavec, ki izpolnjuje starostni pogoj za pridobitev pravice do predčasne pokojnine po 29. členu ZPIZ-2</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A019" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Delavec, za katerega delodajalec uveljavlja vračilo prispevkov po 157. členu ZPIZ-2</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A020" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Pogodba za določen čas (39. čl. ZUTD-A)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A021" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Pogodba za nedoločen čas (39. čl. ZUTD-A)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A022" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Delavec, za katerega delodajalec uveljavlja oprostitev plačila prispevkov po ZIUTDSV ali po ZIUPTD</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A023" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Status invalida</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A024" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Ni zavarovanec po 15. ali 20. čl. ZZVZZ</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A025" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Uporaba zakonodaje druge države za plačilo prispevkov</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A026" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Vajenec</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="1"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="A027" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Vajenec po 36. in 37. čl. ZVaj</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A028" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A028_01" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek - januar</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A028_02" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek - februar</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A028_03" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek - marec</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A028_04" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek - april</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A028_05" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek - maj</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A028_06" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek - junij</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A028_07" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek - julij</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A028_08" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek - avgust</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A028_09" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek - september</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A028_10" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek - oktober</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A028_11" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek - november</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A028_12" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek - december</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="RealEstate" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>Podatki o nepremičninah</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="A030KO" minOccurs="0" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>Katastrska občina</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="A030PS" minOccurs="0" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>Številka parcele</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="A030SS" minOccurs="0" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>Številka stavbe</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="A030Z" minOccurs="0" type="NegativeDecimal14_2Type">
                      <xs:annotation>
                        <xs:documentation>Znesek</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="IncomeTax" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>Tabela za dohodnino</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="A051" minOccurs="1" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>Dohodninska vrsta dohodka</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="A052" minOccurs="1" type="NegativeDecimal14_2Type">
                      <xs:annotation>
                        <xs:documentation>Dohodek</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="A053" minOccurs="0" type="NegativeDecimal14_2Type">
                      <xs:annotation>
                        <xs:documentation>Normirani stroški/odhodki</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="Contributions" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>Tabela za prispevke</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="A061" minOccurs="1" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Vrsta osnove za prispevke</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="A062" minOccurs="1" type="NegativeDecimal14_2Type">
                      <xs:annotation>
                        <xs:documentation>Znesek</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="A071S" type="NegativeDecimal14_2Type" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za pokojninsko in invalidsko zavarovanje - stopnja</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A071O" minOccurs="0" type="NegativeDecimal14_2Type">
                <xs:annotation>
                  <xs:documentation>Prispevek za pokojninsko in invalidsko zavarovanje - obračunani znesek</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A071P" minOccurs="0" type="NegativeDecimal14_2Type">
                <xs:annotation>
                  <xs:documentation>Prispevek za pokojninsko in invalidsko zavarovanje - plačani znesek</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A072S" type="NegativeDecimal14_2Type" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za zdravstveno zavarovanje - stopnja</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A072O" minOccurs="0" type="NegativeDecimal14_2Type">
                <xs:annotation>
                  <xs:documentation>Prispevek za zdravstveno zavarovanje - obračunani znesek</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A072P" minOccurs="0" type="NegativeDecimal14_2Type">
                <xs:annotation>
                  <xs:documentation>Prispevek za zdravstveno zavarovanje - plačani znesek</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A072aSZ" type="NegativeDecimal14_2Type" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Obvezni zdravstveni prispevek - pavšal</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A072aO" minOccurs="0" type="NegativeDecimal14_2Type">
                <xs:annotation>
                  <xs:documentation>Obvezni zdravstveni prispevek - obračunani znesek</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A073S" type="NegativeDecimal14_2Type" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za starševsko varstvo - stopnja</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A073O" minOccurs="0" type="NegativeDecimal14_2Type">
                <xs:annotation>
                  <xs:documentation>Prispevek za starševsko varstvo - obračunani znesek</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A073P" minOccurs="0" type="NegativeDecimal14_2Type">
                <xs:annotation>
                  <xs:documentation>Prispevek za starševsko varstvo - plačani znesek</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A074S" type="NegativeDecimal14_2Type" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za primer brezposelnosti - stopnja</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A074O" minOccurs="0" type="NegativeDecimal14_2Type">
                <xs:annotation>
                  <xs:documentation>Prispevek za primer brezposelnosti - obračunani znesek</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A074P" minOccurs="0" type="NegativeDecimal14_2Type">
                <xs:annotation>
                  <xs:documentation>Prispevek za primer brezposelnosti - plačani znesek</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A075O" minOccurs="0" type="NegativeDecimal14_2Type">
                <xs:annotation>
                  <xs:documentation>Skupaj obračunani - obračunani znesek</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A075P" minOccurs="0" type="NegativeDecimal14_2Type">
                <xs:annotation>
                  <xs:documentation>Skupaj plačani - plačani znesek</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A081S1" type="NegativeDecimal14_2Type" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za pokojninsko in invalidsko zavarovanje - stopnja 1</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A081S2" type="NegativeDecimal14_2Type" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za pokojninsko in invalidsko zavarovanje - stopnja 2</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A081SZ" type="NegativeDecimal14_2Type" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za pokojninsko in invalidsko zavarovanje - pavšal</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A081SF" type="xs:boolean" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za pokojninsko in invalidsko zavarovanje - true=izpolnjeno A081SZ, false=izpolnjeno A081S</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A081O" minOccurs="0" type="NegativeDecimal14_2Type">
                <xs:annotation>
                  <xs:documentation>Prispevek za pokojninsko in invalidsko zavarovanje - obračunani znesek</xs:documentation>
                </xs:annotation>
