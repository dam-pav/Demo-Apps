<?xml version="1.0" encoding="UTF-8"?>
<xs:schema id="REK_O_Analytical_1"
           targetNamespace="http://edavki.durs.si/Documents/Schemas/REK_O_Analytical_1.xsd"
           elementFormDefault="qualified"
           xmlns="http://edavki.durs.si/Documents/Schemas/REK_O_Analytical_1.xsd"
           xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:element name="AnalyticalData">
    <xs:annotation>
      <xs:documentation>Individualni del REK obrazca</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="AnalyticalDataItem" minOccurs="0" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation>Individualni del REK obrazca</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>

              <!--PODATKI O PREJEMNIKU DOHODKA-->

              <xs:element name="A001" minOccurs="0" type="xs:int">
                <xs:annotation>
                  <xs:documentation>Davčna številka</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A002" minOccurs="0" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Identifikacijska številka</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A003" minOccurs="1" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Ime</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A003a" minOccurs="0" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Priimek</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A004" minOccurs="1">
                <xs:annotation>
                  <xs:documentation>Oznaka za rezidentstvo</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="1"/>
                    <xs:enumeration value="R">
                      <xs:annotation>
                        <xs:documentation>Rezident</xs:documentation>
                      </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="N">
                      <xs:annotation>
                        <xs:documentation>Nerezident</xs:documentation>
                      </xs:annotation>
                    </xs:enumeration>
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="A004a" minOccurs="0" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Država rezidenstva</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A004b" minOccurs="0" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Naslov nerezidenta - ulica</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A004c" minOccurs="0" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Naslov nerezidenta - kraj</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A004d" minOccurs="0" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Naslov nerezidenta - država</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A011" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Napotitev na delo v tujino (2. odst. 144. čl. ZPIZ-2)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A011a" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Napotitev na čezmejno opravljanje dela (45. a čl. ZDoh-2)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A011b" minOccurs="0" type="xs:date">
                <xs:annotation>
                  <xs:documentation>Datum prve napotitve</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A011c" minOccurs="0" type="xs:int">
                <xs:annotation>
                  <xs:documentation>Zaporedna št. meseca</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A012" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Napotitev na delo v tujino - javni uslužbenec</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A014" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Invalid nad predpisano kvoto</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A017" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Dopolnjena starost 60 let (156. čl. ZPIZ-2)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A020" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Pogodba za določen čas (39. čl. ZUTD-A)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A021" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Pogodba za nedoločen čas (39. čl. ZUTD-A)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A024" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Ni zavarovanec po 15. ali 20. čl. ZZVZZ</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A025" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Uporaba zakonodaje druge države za plačilo prispevkov</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A025a" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Zavarovanec po 18. čl. ZPIZ-2</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A026" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Vajenec - izbira letnika</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="1"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="A027" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Vajenec po 36. in 37. čl. Zvaj</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A028" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A028_01" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek - januar</xs:documentation>
                </xs:annotation>                                                   
              </xs:element>                                                        
              <xs:element name="A028_02" minOccurs="0" type="xs:boolean">          
                <xs:annotation>                                                    
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek - februar</xs:documentation>
                </xs:annotation>                                                   
              </xs:element>                                                        
              <xs:element name="A028_03" minOccurs="0" type="xs:boolean">          
                <xs:annotation>                                                    
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek - marec</xs:documentation>
                </xs:annotation>                                                   
              </xs:element>                                                        
              <xs:element name="A028_04" minOccurs="0" type="xs:boolean">          
                <xs:annotation>                                                    
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek - april</xs:documentation>
                </xs:annotation>                                                   
              </xs:element>                                                        
              <xs:element name="A028_05" minOccurs="0" type="xs:boolean">          
                <xs:annotation>                                                    
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek - maj</xs:documentation>
                </xs:annotation>                                                   
              </xs:element>                                                        
              <xs:element name="A028_06" minOccurs="0" type="xs:boolean">          
                <xs:annotation>                                                    
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek - junij</xs:documentation>
                </xs:annotation>                                                   
              </xs:element>                                                        
              <xs:element name="A028_07" minOccurs="0" type="xs:boolean">          
                <xs:annotation>                                                    
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek - julij</xs:documentation>
                </xs:annotation>                                                   
              </xs:element>                                                        
              <xs:element name="A028_08" minOccurs="0" type="xs:boolean">          
                <xs:annotation>                                                    
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek - avgust</xs:documentation>
                </xs:annotation>                                                   
              </xs:element>                                                        
              <xs:element name="A028_09" minOccurs="0" type="xs:boolean">          
                <xs:annotation>                                                    
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek - september</xs:documentation>
                </xs:annotation>                                                   
              </xs:element>                                                        
              <xs:element name="A028_10" minOccurs="0" type="xs:boolean">          
                <xs:annotation>                                                    
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek - oktober</xs:documentation>
                </xs:annotation>                                                   
              </xs:element>                                                        
              <xs:element name="A028_11" minOccurs="0" type="xs:boolean">          
                <xs:annotation>                                                    
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek - november</xs:documentation>
                </xs:annotation>                                                   
              </xs:element>                                                        
              <xs:element name="A028_12" minOccurs="0" type="xs:boolean">          
                <xs:annotation>                                                    
                  <xs:documentation>Zavarovanec za obvezni zdravstveni prispevek - december</xs:documentation>
                </xs:annotation>
              </xs:element>

              <!--PODATKI O NEPREMIČNINAH-->

              <xs:element name="RealEstate" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>Podatki o nepremičnininah</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="A030KO" minOccurs="0" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>Katastrska občina</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="A030PS" minOccurs="0" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>Številka parcele</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="A030SS" minOccurs="0" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>Številka stavbe</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="A030Z" minOccurs="0" type="Decimal_14_2">
                      <xs:annotation>
                        <xs:documentation>Znesek</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>

              <!--BRUTO DOHODEK IN DAVČNA OSNOVA-->

              <xs:element name="IncomeTax" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>Bruto dohodek in davčna osnova</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="A051" minOccurs="1" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>Dohodninska vrsta dohodka</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="A052" minOccurs="0" type="Decimal_14_2">
                      <xs:annotation>
                        <xs:documentation>Dohodek</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="A052a" minOccurs="0" type="Decimal_14_2">
                      <xs:annotation>
                        <xs:documentation>Dohodek, ki se všteva v davčno osnovo</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="A053" minOccurs="0" type="Decimal_14_2">
                      <xs:annotation>
                        <xs:documentation>Normirani stroški/odhodki</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>

              <xs:element name="A052b" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                  <xs:documentation>Dohodek, od katerega se ne odtegne davčni odtegljaj</xs:documentation>
                </xs:annotation>
              </xs:element>

              <xs:element name="A054" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>Bruto dohodek in davčna osnova</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="A054a" minOccurs="1" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>Oznaka dohodka (polja B), ki se všteva v davčno osnovo</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="A054z" minOccurs="0" type="Decimal_14_2">
                      <xs:annotation>
                        <xs:documentation>Znesek dohodka, ki se všteva v davčno osnovo</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>

              <!--OSNOVA ZA OBRAČUN IN PLAČILO PRISPEVKOV-->

              <xs:element name="Contribution" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>Osnova za obračun in plačilo prispevkov</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="A061" minOccurs="1" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>Vrsta osnove za prispevke</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="A062" minOccurs="0" type="Decimal_14_2">
                      <xs:annotation>
                        <xs:documentation>Osnova za prispevke - Znesek</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="A062a" minOccurs="0" type="xs:int">
                      <xs:annotation>
                        <xs:documentation>Osnova za prispevke - Ure</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>

              <!--PRISPEVKI ZA SOCIALNO VARNOST DELOJEMALCA-->
              
              <xs:element name="A071O" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za pokojninsko in invalidsko zavarovanje - Obračunani</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A071P" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za pokojninsko in invalidsko zavarovanje - Za plačilo</xs:documentation>
                </xs:annotation>
              </xs:element>

              <xs:element name="A072O" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za zdravstveno zavarovanje - Obračunani</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A072P" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za zdravstveno zavarovanje - Za plačilo</xs:documentation>
                </xs:annotation>
              </xs:element>

              <xs:element name="A072aO" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Obvezni zdravstveni prispevek - Obračunani</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A072aP" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Obvezni zdravstveni prispevek - Za plačilo</xs:documentation>
                </xs:annotation>
              </xs:element>

              <xs:element name="A073O" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za starševsko varstvo - Obračunani</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A073P" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za starševsko varstvo - Za plačilo</xs:documentation>
                </xs:annotation>
              </xs:element>

              <xs:element name="A074O" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za primer brezposelnosti - Obračunani</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A074P" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za primer brezposelnosti - Za plačilo</xs:documentation>
                </xs:annotation>
              </xs:element>

              <xs:element name="A075O" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Skupaj obračunani</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A075P" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Skupaj za plačilo</xs:documentation>
                </xs:annotation>
              </xs:element>

              <!--PRISPEVKI ZA SOCIALNO VARNOST DELODAJALCA-->
              
              <xs:element name="A081O" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za pokojninsko in invalidsko zavarovanje - Obračunani</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A081P" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za pokojninsko in invalidsko zavarovanje - Za plačilo</xs:documentation>
                </xs:annotation>
              </xs:element>

              <xs:element name="A082O" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za zdravstveno zavarovanje - Obračunani</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A082P" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za zdravstveno zavarovanje - Za plačilo</xs:documentation>
                </xs:annotation>
              </xs:element>

              <xs:element name="A083O" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za starševsko varstvo - Obračunani</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A083P" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za starševsko varstvo - Za plačilo</xs:documentation>
                </xs:annotation>
              </xs:element>

              <xs:element name="A084O" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za primer brezposelnosti - Obračunani</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A084P" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za primer brezposelnosti - Za plačilo</xs:documentation>
                </xs:annotation>
              </xs:element>

              <xs:element name="A085O" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Poškodbe pri delu - Obračunani</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A085P" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Poškodbe pri delu - Za plačilo</xs:documentation>
                </xs:annotation>
              </xs:element>

              <xs:element name="A086O" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Skupaj obračunani</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A086P" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Skupaj za plačilo</xs:documentation>
                </xs:annotation>
              </xs:element>

              <!--PRISPEVKI ZA ZAVAROVALNO DOBO, KI SE ŠTEJEJO S POVEČANJEM-->

              <xs:element name="A101P" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Prispevek za zavarovalno dobo s povečanjem - Za plačilo</xs:documentation>
                </xs:annotation>
              </xs:element>

              <!--UVELJAVLJANJE UGODNOSTI IZ MEDNARODNIH POGODB-->

              <xs:element name="D11" type="xs:boolean" minOccurs="0" default="false">
                <xs:annotation>
                  <xs:documentation>Uveljavljanje ugodnosti iz mednarodnih pogodb</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="D11S" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Uveljavljanje ugodnosti iz mednarodnih pogodb - Stopnja</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="D11N" type="xs:string" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Uveljavljanje ugodnosti iz mednarodnih pogodb - Številka odločbe/potrdila</xs:documentation>
                </xs:annotation>
              </xs:element>

              <xs:element name="D12" type="xs:boolean" minOccurs="0" default="false">
                <xs:annotation>
                  <xs:documentation>Uveljavljanje znižane stopnje</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="D12S" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Uveljavljanje znižane stopnje - Stopnja</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="D12N" type="xs:string" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Uveljavljanje znižane stopnje - Številka odločbe/potrdila</xs:documentation>
                </xs:annotation>
              </xs:element>

              <!--DAVČNI ODTEGLJAJ-->

              <xs:element name="A090" type="xs:boolean" minOccurs="0" default="false">
                <xs:annotation>
                  <xs:documentation>Davčni odtegljaj - Drugi delodajalec</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A091" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Davčni odtegljaj - Davčni odtegljaj</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A092" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Davčni odtegljaj - Odbitek davka plačanega v tujini</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="A093" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Davčni odtegljaj - Skupaj</xs:documentation>
                </xs:annotation>
              </xs:element>

              <!--DODATNI PODATKI - DOHODKI IZ DELOVNEGA RAZMERJA-->

              <xs:element name="B04" type="Decimal_14_2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Povračilo stroškov do uredbe vlade - prehrana</xs:documentation>
                </xs:annotation>
              </xs:element>
